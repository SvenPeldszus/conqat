<conqat xmlns="http://conqat.cs.tum.edu/ns/config" >
 
 <block-spec name="execution-02-child1">

<param name="part1"><attr name="value" /></param>
<param name="part2"><attr name="value" /></param>
 
<processor name="string" class="org.conqat.engine.core.driver.processors.MultiAttributeStringGenerator">
	<set part1="@part1.value" part2="@part2.value" />
</processor>

<out name="result" ref="@string" />

</block-spec>

</conqat>

